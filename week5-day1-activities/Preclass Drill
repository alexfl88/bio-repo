function maxArray2(arr) {
    const newArr = arr.sort();
    return arr[arr.length-1];
}

function maxArray3(arr) {
    let output = arr[0]
    for (let i = 1; i < arr.length; index++) {
        if (arr[1] > output) {
            output = arr[i]
        }
    }
    return output;
}

console.log("maxArray1", maxArray1(1,2,5,3));
